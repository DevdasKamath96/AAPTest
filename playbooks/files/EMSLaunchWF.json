    "Inventory": "EMSLaunch",
    "templates": [
        {
            "Node": "EMSLaunchPreReq"
        },
        {
            "Node": "EMSImagePullMsiDev",
            "Parent": ["EMSLaunchPreReq"],
            "NodeType": "success"
