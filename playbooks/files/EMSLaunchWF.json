        {
            "Node": "EMSContainerConfiguration",
            "Parent": ["EMSContainerLaunch"],
            "NodeType": "success"
        },
        {
            "Node": "CreateEMSPasswordless",
            "Parent": ["EMSContainerConfiguration"],
            "NodeType": "success"
        }
